@model Evaluation.UI.Models.CQAf05VM
@{
    ViewData["Title"] = "Businnes";
    ViewData["SubTitle"] = "Indivudual Health";
}
<link href="~/assets/extra-libs/datatables.net-bs4/css/dataTables.bootstrap4.css"
      rel="stylesheet" />
<link rel="stylesheet"
      type="text/css"
      href="~/assets/extra-libs/datatables.net-bs4/css/responsive.dataTables.min.css" />
<style>
    .table-container {
        overflow-x: auto;
    }
</style>
<div class="row">
    <div class="col-6 "> <h2>@Model.ContactVM.FirstName @Model.ContactVM.MiddleName @Model.ContactVM.LastName</h2></div>
    <div class="col-6 ">
        <div class="" style="float: right;">
            <input type="button" class="btn btn-danger" value="Special" id="btnSpecial" />
            <input type="button" class="btn btn-primary" value="Dashboard" id="btnDashboard" />

            <button type="button" id="btnSendEmailExcel" class="
                        btn btn-secondary
                      ">
                <i class="m-1 fas fa-download"></i>Excel Template
            </button>

        </div>
    </div>
</div>
<input type="hidden" id="ContactID" value="@Model.ContactVM.RecId" />
<!-- Nav tabs -->
<ul class="nav nav-tabs" role="tablist">
    <li class="nav-item">
        <a class="nav-link d-flex active" data-bs-toggle="tab" href="#cqdtl" role="tab">
            <span>
                <i class="fas fa-user"></i>
            </span>
            <span class="d-none d-md-block ms-2">CQ Details</span>
        </a>
    </li>
    <li class="nav-item">
        <a class="nav-link d-flex " data-bs-toggle="tab" href="#cqsum" role="tab">
