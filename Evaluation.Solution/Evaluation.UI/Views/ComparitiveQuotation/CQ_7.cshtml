<partial name="_SpecialPrice.cshtml" />
@section Scripts{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

    <script src="~/assets/extra-libs/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/dist/js/pages/datatable/datatable-basic.init.js"></script>
    <script src="~/assets/extra-libs/datatables.net-bs4/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/buttons.flash.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.5.1/js/buttons.html5.min.js"></script>
   
    <script>
        var dataTableDataDetails = @Html.Raw(Json.Serialize(Model.CQDetailsBL051807));
        var dataTableDataSummary = @Html.Raw(Json.Serialize(Model.CQSummaryBL051807));

        var datapivot = @Html.Raw(Json.Serialize(Model.CQPivotList));
        var dataBenefit = @Html.Raw(Json.Serialize(Model.CQBenefitList));
        var dataBills = @Html.Raw(Json.Serialize(Model.CQBillsList));
        var dataHeader = @Html.Raw(Json.Serialize(Model.CQHeader));
        var dataCategory = @Html.Raw(Json.Serialize(Model.CQCategory));

    </script>
    <script src="~/js/cq/shared.js"></script>

